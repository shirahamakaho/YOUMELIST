<h1>Profile_editpage</h1>
<%= form_with model:@user,url:user_path(@user),method: :patch do |f| %>
<%= f.label:name %>
<%= f.text_field:name %><br>
<%= f.label:introduction %>
<%= f.text_area:introduction %>
<%= f.submit %>
<% end %>
<p>Your E-mail</p>
<%= @user.email %>
<%= @user.is_deleted %>
<p>password_edit</p>
